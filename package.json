{
  "name": "nuxt-sample-module",
  "version": "6.0.0-rc.0",
  "description": "Testing code",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/batopa/nuxt-sample-module.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run prepack && changelogen --release && git push --follow-tags",
    "release:patch": "npm run prepack && changelogen --release --patch --push",
    "release:minor": "npm run prepack && changelogen --release --minor --push",
    "release:major": "npm run prepack && changelogen --release --major --push",
    "release:alpha": "npm run prepack && changelogen --release --premajor alpha --push",
    "release:beta": "npm run prepack && changelogen --release --premajor beta --push",
    "release:rc": "npm run prepack && changelogen --release --premajor rc --push",
    "prerelease:alpha": "npm run prepack && changelogen --release --prerelease alpha --push",
    "prerelease:beta": "npm run prepack && changelogen --release --prerelease beta --push",
    "prerelease:rc": "npm run prepack && changelogen --release --prerelease rc --push",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.8.1",
    "@types/node": "^20.10.3",
    "changelogen": "^0.6.2",
    "eslint": "^8.55.0",
    "nuxt": "^3.8.2",
    "vitest": "^0.33.0"
  }
}
